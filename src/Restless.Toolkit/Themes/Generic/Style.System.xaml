<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:keys="clr-namespace:Restless.Toolkit.Resource"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    <!-- Resource dictionary for default styles only -->
    <!-- These do not have a corresponding custom control -->

    <sys:Double x:Key="{x:Static keys:ResourceKeys.MinHeaderHeightKey}">28</sys:Double>
    <Thickness x:Key="{x:Static keys:ResourceKeys.DefaultHeaderPaddingKey}">4</Thickness>

    <sys:Double x:Key="{x:Static keys:ResourceKeys.MinDataGridRowHeightKey}">34</sys:Double>
    <sys:Double x:Key="{x:Static keys:ResourceKeys.MinTextBoxHeightKey}">32</sys:Double>
    <Thickness x:Key="{x:Static keys:ResourceKeys.TextBoxBorderThicknessKey}">1.25</Thickness>
    

</ResourceDictionary>